<?xml version="1.0" encoding="UTF-8"?>
<module>
  <displayName>StaticModule</displayName>
  <description>A module which does not have DLLs. For test only.</description>

  <!-- Configure client menu which will be should in all client pages. -->
  <clientMenu>
    <item>
      <name>StaticModule</name>
      <url>/static_module/</url>
      <orderNumber>95</orderNumber>
    </item>
  </clientMenu>

  <!-- Map static resources. -->
  <staticResources>
    <staticResource>
      <directory>web/</directory>
      <contextPath>/static_module</contextPath>
    </staticResource>
  </staticResources>
</module>